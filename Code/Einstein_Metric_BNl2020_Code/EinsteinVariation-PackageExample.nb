Cell["\<\
As another example, take the Schwarzschild metric - it has line element:\
\>", "Text",
 CellChangeTimes->{{3.488656596588189*^9, 
  3.488656610019766*^9}},ExpressionUUID->"bd9fc1fe-c12d-4fd0-b062-\
349a70945fa2"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"ds2", " ", "=", " ", 
  RowBox[{
   RowBox[{
    RowBox[{"-", 
     RowBox[{"(", 
      RowBox[{"1", "-", 
       RowBox[{"2", 
        RowBox[{"M", "/", "r"}]}]}], ")"}]}], " ", 
    RowBox[{"dt", "^", "2"}]}], " ", "+", " ", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "-", 
       RowBox[{"2", 
        RowBox[{"M", "/", "r"}]}]}], ")"}], "^", 
     RowBox[{"(", 
      RowBox[{"-", "1"}], ")"}]}], " ", 
    RowBox[{"dr", "^", "2"}]}], " ", "+", " ", 
   RowBox[{
    RowBox[{"r", "^", "2"}], " ", 
    RowBox[{"dT", "^", "2"}]}], " ", "+", " ", 
   RowBox[{
    RowBox[{"r", "^", "2"}], " ", 
    RowBox[{
     RowBox[{"Sin", "[", "T", "]"}], "^", "2"}], " ", 
    RowBox[{"dP", "^", "2"}]}]}]}]], "Input",
 CellChangeTimes->{{3.488656610906516*^9, 
  3.488656627099736*^9}},ExpressionUUID->"aba41352-8206-4bb4-acd1-\
4158e1f71c5a"],

Cell[BoxData[
 RowBox[{
  FractionBox[
   SuperscriptBox["dr", "2"], 
   RowBox[{"1", "-", 
    FractionBox[
     RowBox[{"2", " ", "M"}], "r"]}]], "+", 
  RowBox[{
   SuperscriptBox["dt", "2"], " ", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"-", "1"}], "+", 
     FractionBox[
      RowBox[{"2", " ", "M"}], "r"]}], ")"}]}], "+", 
  RowBox[{
   SuperscriptBox["dT", "2"], " ", 
   SuperscriptBox["r", "2"]}], "+", 
  RowBox[{
   SuperscriptBox["dP", "2"], " ", 
   SuperscriptBox["r", "2"], " ", 
   SuperscriptBox[
    RowBox[{"Sin", "[", "T", "]"}], "2"]}]}]], "Output",
